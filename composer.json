{
    "name": "sgpatil/laravel-varnish",
    "description": "Making Varnish and Laravel play nice together",
    "keywords": [
        "spatie",
        "laravel-varnish"
    ],
    "homepage": "https://github.com/spatie/laravel-varnish",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "5.2.*",
        "illuminate/http": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "^3.3.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Varnish\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Varnish\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
